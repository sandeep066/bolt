# LLM Provider Configuration
# Choose 'openai', 'anthropic', or 'gemini'
LLM_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (alternative to OpenAI)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini Configuration (alternative to OpenAI/Anthropic)
# GEMINI_API_KEY=your_gemini_api_key_here

# LiveKit Configuration for Voice Interviews
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
LIVEKIT_WS_URL=wss://your-livekit-server.com

# Server Configuration
PORT=3001
VITE_API_URL=http://localhost:3001/api

# Optional: JWT Secret for additional security
JWT_SECRET=your_jwt_secret_for_additional_security